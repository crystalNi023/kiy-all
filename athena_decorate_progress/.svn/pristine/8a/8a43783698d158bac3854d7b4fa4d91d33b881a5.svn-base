html,body{
	padding: 0;
	margin: 0;
}
.hea_wid{
	width: 100%;
	height: 90px;
	background: rgb(5, 118, 220);
}

.hea_end_wid{
	width: 95%;
	height: 100%;
	margin: auto;
}
.logo{
	height: 100%;
	width: auto;
	line-height: 90px;
	background: none;
}
.logo img{
	width: 50px;
	vertical-align: middle;
}
.logo span{
	font-size: 20px;
	color: #FFFFFF;
	font-weight: bold;
}
.hea_nav{
	height: 100%;
}
.hea_nav div{
	height: 30px;
	margin: auto 20px;
	margin-top: 35px;
	color: #FFFFFF;
}
.hea_border{
	border-bottom: 1px solid #FFFFFF;
}
.hea_nav a{
	display: inline-block;
    width: 100%;
    height: 100%;
    }
.exit{
	font-size: 14px;
	color: #FFFFFF;
	margin-top: 35px;
}
.end{
	bottom: 0;
}
