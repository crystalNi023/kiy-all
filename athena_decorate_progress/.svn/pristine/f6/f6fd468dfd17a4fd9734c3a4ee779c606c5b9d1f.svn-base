body{
	background: url(/public/img/3.png) no-repeat;
	background-position-y: 170px;
	background-size: 100%;
}
.logo{
	width: 100%;
	height: 115px;
	font-size: 30px;
	line-height: 115px;
}
.logo span{
	margin-left: 38px;
}
.login_register {
	float: right;
	width: 40%;
	height: auto;
	background-image: linear-gradient( #ffffff, #ffffff), linear-gradient( #eff6fe, #eff6fe);
	background-blend-mode: normal, normal;
	box-shadow: 0px 0px 27px 0px rgba(173, 173, 174, 0.46);
	border-radius: 2px;
	border: solid 1px rgba(68, 82, 213, 0.28);
}

.login_margin {
	margin-left: 33px;
	margin-right: 33px;
}

.user_tip {
	margin-top: 68px;
}

.tip_zh {
	font-size: 24px;
	font-weight: normal;
	font-stretch: normal;
	line-height: 18px;
	letter-spacing: 0px;
	color: #282828;
}

.tip-en {
	font-size: 14px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #01a7c1;
}

.user_name {
	width: 100%;
	height: 50px;
	box-sizing: content-box;
	border: solid 1px #cecbcb;
	margin-top: 40px;
}

.user_pwd {
	width: 100%;
	height: 50px;
	box-sizing: content-box;
	border: solid 1px #cecbcb;
	margin-top: 30px;
}

.user_name i {
	display: inline-block;
	margin-top: 10px;
	margin-left: 10px;
	width: 10%;
}

.user_pwd i {
	display: inline-block;
	margin-top: 10px;
	margin-left: 10px;
	width: 10%;
}

.user_name input {
	width: 80%;
	height: 100%;
	font-size: 20px;
	vertical-align: top;
	box-sizing: border-box;
}

.user_pwd input {
	width: 80%;
	height: 100%;
	font-size: 20px;
	vertical-align: top;
	box-sizing: border-box;
}

.y_or_n {
	margin-top: 30px;
	line-height: 32px;
}
.login_a{
	font-size: 16px;	
}
.forget_pwd{
	color: #337AB7 !important;
}
.register{
	width: 100%;
	text-align: center;
	margin-top: 20px;
	margin-bottom: 20px;
}
.y_or_n input {
	display: inline-block;
	width: 26px;
	height: 26px;
	color: black;
	vertical-align: sub;
}

.y_or_n span {
	margin-left: 10px;
	display: inline-block;
	vertical-align: text-bottom;
	height: 26px;
	line-height: 26px;
	font-size: 16px;
	color: #4d4d4d;
}

.login_btn {
	width: 100%;
	height: 57px;
	margin-top: 20px;
	background-blend-mode: normal, normal;
	font-size: 24px;
	color: #FFFFFF;
	text-align: center;
	line-height: 57px;
	border-radius: 2px;
}
.login_btn .login_sub_btn{
	width: 100%;
	height: 57px;
	font-size: 24px;
	color: #FFFFFF;
	background-image: linear-gradient( #029dd5, #029dd5), linear-gradient( #01a7c1, #01a7c1);
}
.contend{
	width: 80%;
	margin: auto;
}
.login_img{
	width: 45%;
}
.login_img img{
	margin-top: 49px;
}
#prompt_msg{
	font-size: 16px;
  color: red;
  text-align: center;
}
.without_user{
	color: #337AB7 !important;
}
