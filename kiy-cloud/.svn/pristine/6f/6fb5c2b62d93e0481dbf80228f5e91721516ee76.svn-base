#服务器名称
NAME=KY Cloud

#线程数量,0由系统决定
#当硬件系统使用了超线程等技术时建议手动设定(THREAD=CPU物理数*核心数*超线程*4)
THREAD=0
#调试模式(将输出调试日志)
DEBUG=ON

#主控服务启用
MASTER=ON
#主控服务地址,空表示所有地址
MASTER_IP=
#主控服务端口
MASTER_PORT=1240

#Http接口服务启用
HTTP=ON
#Http接口服务模式HTTP/HTTPS
HTTP_MODE=HTTP
#Http接口服务绑定IP地址,空表示所有地址
HTTP_IP=
#Http接口服务绑定端口0~65535,默认80
HTTP_PORT=8080
#Http接口SSL端口0~65535,默认443
HTTP_SSL_PORT=443

#通知服务启用(邮件通知)
NOTICE=OFF
#发送邮件的地址
MAIL_FOFM=service@cqkiy.com
#邮件服务用户名
MAIL_USER=service
#邮件服务密码
MAIL_PASSWORD=service
#邮件服务主机
MAIL_HOST=mail.cqkiy.com
#邮件服务端口(SMTP)
MAIL_PORT=337
