}
html,body{
}
table {
	width:100%;
	border-collapse:collapse;
	border-spacing:0px 0px;
	border: 1px solid #333;
}
table caption {
	padding:5px;
	font-size: 18px;	
}
table tr {
}
table tr th {
	padding:5px;
	font-size: 12px;
	border: 1px solid #333;
}
table tr td {
	padding:5px;
	font-size: 12px;
	border: 1px solid #333;
}
