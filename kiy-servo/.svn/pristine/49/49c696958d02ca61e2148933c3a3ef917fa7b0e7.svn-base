#伺服器名称
NAME=撒

#线程数量,0由系统决定
#当硬件系统使用了超线程等技术时建议手动设定(THREAD=CPU物理数*核心数*超线程*4)
THREAD=0

DEBUG=ON

#设备服务启用
DRIVER=OFF
#设备中断重启间隔(秒)
#当设备出现异常中断等待此时间后重启
DRIVER_RESTART=6
#设备心跳间隔(秒)
#通过以太网连接的设备当没有通信时发送链路包的触发时间
DRIVER_HEARTBEAT=30
#设备指令间隔(秒)
#当多个用户同时获取设备状态时在此时间内的请求会合并
DRIVER_INTERVAL=4
#设备通信超时(秒)
#等待设备返回指令的时间,超过此时间将视为超时
DRIVER_TIMEOUT=24
#设备通信系数(BAUD_RATE/DELAY)
#串口通信设备在连续发送和转换状态时需要等待一定时间量
DRIVER_DELAY=128

#主控服务启用
MASTER=ON
#主控服务地址,空表示所有地址
MASTER_IP=
#主控服务端口
MASTER_PORT=1230

#计划策略启用
STRATEGY=ON
#设备巡游启用
CRUISE=ON

#MQTT服务启用
MQTT=ON
#MQTT HOST
MQTT_HOST=tcp://127.0.0.1:1883
#MQTT USERNAME
MQTT_USERNAME=root
#MQTT PASSWORD
MQTT_PASSWORD=cqkiy6688
#MQTT 订阅 TOPIC
MQTT_SUBSCRIPTION_TOPIC=kiy/+

#Http接口服务启用
HTTP=ON
#Http接口服务模式HTTP/HTTPS
HTTP_MODE=HTTP
#Http接口服务绑定IP地址,空表示所有地址
HTTP_IP=
#Http接口服务绑定端口0~65535,默认80
HTTP_PORT=8080
#Http接口SSL端口0~65535,默认443
HTTP_SSL_PORT=443

#数据库启用
DB=ON
#数据库最大连接数
DB_MAX=2
#数据库类型MySql/Oracle
DB_TYPE=MySql
#数据库连接字符串
DB_URL=jdbc:mysql://192.168.0.215:3306/kiy-servo?characterEncoding=utf8&useSSL=false
#数据库用户名
DB_USER=root
#数据库密码
DB_PASSWORD=hu110112113
#文件数据启用
FILE_DATA=OFF

#通知服务启用
NOTICE=OFF
#邮件发送服务器
MAIL_HOST=
#邮件发送服务器端口
MAIL_HOST_PORT=25
#邮件服务器登录用户
MAIL_USER=
#邮件服务器登录密码
MAIL_PASSWORD=

#云服务启用
CLOUD=ON
#云服务主机
CLOUD_HOST=cloud.cqkiy.com
#云服务端口
CLOUD_PORT=1230
#云服务重置间隔(当连接中断/异常)
CLOUD_RESET=6
#云服务标识
CLOUD_ID=6709b59a-1645-46b7-8201-762c1b711ae8

#天气城市ID
WEATHER_WEAID=110
#PM城市ID
AQI_WEAID=105

#短信服务
